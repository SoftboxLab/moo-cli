{
  "name": "@softboxlab/moo-cli",
  "version": "0.5.1",
  "description": "A MOOvelous CLI",
  "main": "dist/index.js",
  "bin": {
    "moo-cli": "dist/index.js",
    "moo": "dist/index.js"
  },
  "scripts": {
    "watch": "tsc -w",
    "check": "tsc --noEmit",
    "lint": "tslint -p . -c tslint.json",
    "lintfix": "npm run lint -- --fix",
    "build": "npm run check && npm run lintfix && tsc",
    "release:tags": "git push && git push --tags",
    "release:patch": "npm version patch && npm run release:tags",
    "release:minor": "npm version minor && npm run release:tags",
    "release:major": "npm version major && npm run release:tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SoftboxLab/moo-cli.git"
  },
  "keywords": [
    "cli",
    "command",
    "line",
    "interface",
    "tool",
    "frontend",
    "web",
    "projects",
    "js"
  ],
  "author": "Alex Bruno <alexcaceres@softbox.com.br>, Lucas Vilaboim <lucasvilaboim@softbox.com.br>, Pablo Henrique Penha Silva <pablohpsilva@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SoftboxLab/moo-cli/issues"
  },
  "homepage": "https://github.com/SoftboxLab/moo-cli#readme",
  "lint-staged": {
    "*.{ts}": [
      "npm run lint -- --fix",
      "git add"
    ],
    "packages/**/bin/*": [
      "npm run lint -- --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/inquirer": "0.0.43",
    "@types/mkdirp": "^0.5.2",
    "@types/mustache": "^0.8.31",
    "@types/node": "^10.12.0",
    "chalk": "^2.4.1",
    "conventional-changelog": "^2.0.3",
    "execa": "^1.0.0",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "commander": "^2.19.0",
    "cowsay": "^1.3.1",
    "inquirer": "^6.2.0",
    "mkdirp": "^0.5.1",
    "motivation": "^1.0.3",
    "mustache": "^3.0.0",
    "rimraf": "^2.6.2"
  }
}
