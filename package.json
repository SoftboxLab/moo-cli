{
  "name": "moo-cli",
  "version": "0.0.0-development",
  "description": "A MOOvelous CLI",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "scripts": {
    "watch": "tsc -w",
    "check": "tsc --noEmit",
    "lint": "tslint -p . -c tslint.json",
    "lintfix": "yarn lint --fix",
    "gitrelease": "github-release",
    "build": "npm run check && npm run lint && tsc",
    "test": "jest",
    "precommit": "lint-staged",
    "push": "git commit -a -m",
    "changelog": "node config/git/genChangelog.js run",
    "commitmsg": "node config/git/verifyCommitMsg.js",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "release": {
    "branch": "next"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SoftboxLab/moo-cli.git"
  },
  "keywords": [
    "cli",
    "command",
    "line",
    "interface",
    "tool",
    "frontend",
    "web",
    "projects",
    "js"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SoftboxLab/moo-cli/issues"
  },
  "homepage": "https://github.com/SoftboxLab/moo-cli#readme",
  "lint-staged": {
    "*.{ts}": [
      "npm run lint -- --fix",
      "git add"
    ],
    "packages/**/bin/*": [
      "npm run lint -- --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/jest": "^23.3.1",
    "@types/mustache": "^0.8.31",
    "@types/node": "^10.5.3",
    "@types/shelljs": "^0.8.0",
    "chalk": "^2.4.1",
    "conventional-changelog": "^2.0.1",
    "execa": "^0.10.0",
    "github-release": "^0.2.0",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "lint-staged": "^7.2.0",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.3.1",
    "typescript": "^2.9.2",
    "travis-deploy-once": "^5.0.1",
    "semantic-release": "^15.9.3"
  },
  "dependencies": {
    "commander": "^2.16.0",
    "cowsay": "^1.3.1",
    "inquirer": "^6.0.0",
    "motivation": "^1.0.3",
    "mustache": "^2.3.0",
    "shelljs": "^0.8.2"
  }
}
